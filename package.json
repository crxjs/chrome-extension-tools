{
  "name": "rollup-plugin-manifest-input",
  "version": "0.1.0",
  "description": "Use a Chrome extension manifest in the rollup input field.",
  "license": "MIT",
  "author": "Jack and Amy Steam <jacksteamdev@gmail.com>",
  "files": [
    "build"
  ],
  "repository": "bumble-org/rollup-plugin-manifest-entry.git",
  "main": "build/bundle-cjs.js",
  "module": "build/bundle-esm.js",
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "test": "jest",
    "preversion": "npm run build",
    "postversion": "git push && npm publish"
  },
  "jest": {
    "clearMocks": true,
    "modulePathIgnorePatterns": [
      "fixtures/dest"
    ],
    "setupFilesAfterEnv": [
      "./tests/jest.setup.js"
    ],
    "transform": {
      ".(js|jsx|ts|tsx)": "@sucrase/jest-plugin"
    }
  },
  "dependencies": {
    "@bumble/manifest-entry-points": "^0.3.0",
    "fs-extra": "^7.0.1"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.0.0",
    "eslint": "^5.11.0",
    "jest": "^24.1.0",
    "replace-in-file": "^3.4.4",
    "rollup": "^1.6.0",
    "rollup-pluginutils": "^2.4.1",
    "simple-git": "^1.107.0",
    "sucrase": "^3.9.5"
  },
  "peerDependencies": {
    "rollup": "^1.6.0"
  }
}
