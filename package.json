    "read-pkg-up": "^7.0.1",
    "rollup-plugin-bundle-imports": "^1.4.5",
    "ts-jest": "^25.2.0",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=8.16.0"
  }
