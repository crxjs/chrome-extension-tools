    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "jsonpath-plus": "^4.0.0",
    "lodash.difference": "^4.5.0",
    "lodash.flatten": "^4.4.0",
    "lodash.get": "^4.4.2",
    "mem": "^6.0.1",
    "prettier": "^1.19.1",
    "rollup": "^1.31.0",
    "slash": "^3.0.0",
    "webextension-polyfill": "^0.6.0"
