{
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://a.com/*", "http://b.com/*"]
    }
  ],
  "host_permissions": ["https://c.com/*"],
  "manifest_version": 3,
  "name": "mv3-basic-js",
  "version": "1.0.0",
  "description": "Basic MV3 Chrome Extension"
}
